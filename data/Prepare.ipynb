    "#Convert shapefiles into GeoJSON\n",
    "for i in range(0,6):\n",
    "    shpFilename = \"gadm28_levels/gadm28_adm%s.shp\" % i\n",
    "    geojsonFilename = \"gadm28_levels/gadm28_adm%s.geojson\" % i\n",
    "    cmd = \" ogr2ogr -f GeoJSON %s %s\" % (geojsonFilename, shpFilename)\n",
    "    !$cmd"
